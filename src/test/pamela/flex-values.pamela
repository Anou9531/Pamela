(defpclass flex1 []
  :fields {:number 3.14
           :string {:initial "foo"
                    :access :public}
           :boolean {:initial false
                     :observable true}
           :enum :red})

(defpclass flex2 [a b c d]
  :fields {:number a
           :string b
           :boolean c
           :enum d})

(defpclass test-flex []
  :fields {:flex1 (flex1 :id "flex1" :interface "ZMQ")
           :flex2 (flex2 1000 "elephant" true :blue :id "flex2")})
