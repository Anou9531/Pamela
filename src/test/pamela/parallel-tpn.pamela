(defpclass parallel-tpn [plant]
  :meta {:version "0.2.0"
         :depends [[plant "0.2.0"]]
         :doc "Simple Parallel with 2 Activities"}
  :methods [(defpmethod simple-parallel-tpn
              {:doc "Simple Parallel TPN"}
              []
              (parallel
                (plant.bounded)
                (plant.unbounded))
              )])
